{
    "manifest_version": 3,
    "name": "TaskMate ",
    "version": "1.0",
    "description": "A simple to-do list Chrome extension.",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.jpg",
            "48": "icons/icon48.jpg",
            "128": "icons/icon128.jpg"
        }
    },
    "icons": {
        "16": "icons/icon16.jpg",
        "48": "icons/icon48.jpg",
        "128": "icons/icon128.jpg"
    }
}
